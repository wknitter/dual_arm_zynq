<?xml version="1.0" encoding="UTF-8"?>
<sdx:platform sdx:vendor="xilinx.com"
              sdx:library="sdx"
              sdx:name="dual_arm_zynq"
              sdx:version="1.0"
              sdx:schemaVersion="1.0"
              xmlns:sdx="http://www.xilinx.com/sdx">
    <sdx:description>
dual_arm_zynq
    </sdx:description>
    <sdx:systemConfigurations sdx:defaultConfiguration="dual_arm_zynq">
        <sdx:configuration sdx:name="dual_arm_zynq"
                           sdx:displayName="dual_arm_zynq"
                           sdx:defaultProcessorGroup="ps7_cortex9_1"
                           sdx:runtimes="cpp">
            <sdx:description>dual_arm_zynq</sdx:description>
            <sdx:bootImages sdx:default="standard">
                <sdx:image sdx:name="standard"
                            sdx:bif="dual_arm_zynq/boot/dual_arm_zynq.bif"
                            sdx:readme="dual_arm_zynq/boot/generic.readme"
                            sdx:qemuArguments="dual_arm_zynq/qemu/qemu_args.txt"
                            />
                <sdx:image sdx:name="standalone_domain"
                            sdx:bif="dual_arm_zynq/boot/dual_arm_zynq.bif"
                            sdx:readme="dual_arm_zynq/boot/generic.readme"
                            sdx:qemuArguments="dual_arm_zynq/standalone_domain/qemu/qemu_args.txt"
                            />
                <sdx:image sdx:name="ps7_cortex9_1"
                            sdx:bif="dual_arm_zynq/boot/dual_arm_zynq.bif"
                            sdx:readme="dual_arm_zynq/boot/generic.readme"
                            sdx:qemuArguments="dual_arm_zynq/ps7_cortex9_1/qemu/qemu_args.txt"
                            />
            </sdx:bootImages>
            <sdx:processorGroup sdx:name="standalone_domain"
                                sdx:displayName="standalone on ps7_cortexa9_0"
                                sdx:cpuType="cortex-a9"
                                sdx:cpuInstance="ps7_cortexa9_0">
                <sdx:os sdx:name="standalone"
                        sdx:displayName="standalone"
                        sdx:bspLibraryPaths="dual_arm_zynq/standalone_domain/bsplib/lib"
                        sdx:bspIncludePaths="dual_arm_zynq/standalone_domain/bspinclude/include"
                        sdx:bspConfig="dual_arm_zynq/standalone_domain/system.mss"
                />
            </sdx:processorGroup>
            <sdx:processorGroup sdx:name="ps7_cortex9_1"
                                sdx:displayName="standalone on ps7_cortexa9_1"
                                sdx:cpuType="cortex-a9"
                                sdx:cpuInstance="ps7_cortexa9_1">
                <sdx:os sdx:name="standalone"
                        sdx:displayName="standalone"
                        sdx:bspLibraryPaths="dual_arm_zynq/ps7_cortex9_1/bsplib/lib"
